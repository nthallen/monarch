%{
  #include "subbuspp.h"

  subbuspp *sb_DACS;

  void DACS_init() {
    sb_DACS = new subbuspp("subbus", 0);
    sb_DACS->load();
  }

  void tick_sic() { if (sb_DACS->fd >= 0) sb_DACS->tick_sic(); }
  void disarm_sic() { if (sb_DACS->fd >= 0) sb_DACS->disarm_sic(); }
%}
TM INITFUNC DACS_init();

